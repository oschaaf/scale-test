nighthawk_client --concurrency 2 --request-header "Host: app-1.plain.apps.ocp.scalelab" \
    "https://192.168.222.20/proxy?p=1&url=http://app-1:8080/mersennePrime?p=300" --no-duration --rps 7000 \
    --max-pending-requests 1000 --max-active-requests 200000 --connections 125 --failure-predicate "a:1" \
    --request-header "x-variant: stable" 2>&1 > app-1.txt &

