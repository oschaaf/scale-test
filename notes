VERSION=2.0 ansible-playbook -v -i hosts.scalelab setup.yaml 
VERSION=2.0 ansible-playbook -v -i hosts.scalelab test.yaml 

nighthawk_client --concurrency 2 --request-header "Host: app-1.mesh.apps.performance.maistra.test" "https://192.168.222.19:8443/proxy?p=1&url=http://app-1:8080/mersennePrime?p=300" --no-duration --rps 600 --max-pending-requests 5000 --max-active-requests 200000 --connections 500 --failure-predicate "a:1" --request-header "x-variant: stable" --experimental-h2-use-multiple-connections --open-loop --h2
nighthawk_client --concurrency 2 --request-header "Host: app-3.plain.apps.ocp.scalelab" "https://192.168.222.19/proxy?p=1&url=http://app-3:8080/mersennePrime?p=300" --no-duration --rps 7000 --max-pending-requests 1000 --max-active-requests 200000 --connections 125 --failure-predicate "a:1" --request-header "x-variant: stable"


rm app-1.txt
rm app-2.txt 
